{
  "icu4c": {
    "git_repo_url": "https://github.com/unicode-org/icu",
    "version": {
      "major": 73,
      "minor": 1
    },
    "library": {
      "type": "static",
      "list": [
        "uc",
        "tu",
        "i18n",
        "io",
        "data"
      ],
      "bits": "nochange",
      "suffix": "__floris",
      "cmake_namespace": "FlorisICU"
    },
    "features": {
      "release": true,
      "debug": false,
      "draft": false,
      "extras": false,
      "auto-cleanup": false,
      "fuzzer": false,
      "icuio": true,
      "layoutex": false,
      "plugins": false,
      "renaming": false,
      "samples": false,
      "strict": true,
      "tests": false,
      "tracing": false,
      "tools": true
    },
    "data": {
      "packaging": "archive",
      "filter_file": "./src/data-feature-filter.json"
    },
    "switches": {
      "UCONFIG_ONLY_COLLATION": 0,
      "UCONFIG_NO_LEGACY_CONVERSION": 0,
      "UCONFIG_NO_BREAK_ITERATION": 0,
      "UCONFIG_NO_COLLATION": 0,
      "UCONFIG_NO_FORMATTING": 0,
      "UCONFIG_NO_TRANSLITERATION": 0,
      "UCONFIG_NO_REGULAR_EXPRESSIONS": 0
    }
  },
  "prebuilt": {
    "install_dir": "./prebuilt",
    "targets": {
      "android": {
        "api": 24,
        "abi_list": [
          "armeabi-v7a",
          "arm64-v8a",
          "x86",
          "x86_64"
        ]
      },
      "desktop": {
        "triple_list": [
          "x86_64-linux-gnu",
          "aarch64-linux-gnu",
          "x86_64-apple-darwin",
          "armv8a-apple-darwin"
        ]
      }
    }
  }
}
